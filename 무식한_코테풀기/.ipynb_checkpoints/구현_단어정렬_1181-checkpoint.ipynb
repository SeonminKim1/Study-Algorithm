   "source": [
    "# Tuple 정렬하면 원소 차례대로 정렬 됨.\n",
    "N = int(input())\n",
    "word = []\n",
    "for _ in range(N):\n",
    "    w = input()\n",
    "    word.append((len(w), w))\n",
    "\n",
    "word = set(word)\n",
    "word = sorted(word)\n",
    "for k, v in word:\n",
    "    print(v)\n"
   ]
