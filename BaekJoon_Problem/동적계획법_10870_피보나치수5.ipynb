    "\n",
    "n = int(input())\n",
    "\n",
    "answer = [0,1]\n",
    "def fibo(n):\n",
    "    if n<=1:\n",
    "        return answer[n]\n",
    "    \n",
    "    else:\n",
    "        for i in range(2, n+1):\n",
    "            answer.append(sum(answer[-2:]))\n",
    "        return answer[n]\n",
    "    \n",
    "print(fibo(n))"
